{
  "swagger": "2.0",
  "info": {
    "description": "Part of comp9322 Assignment, API on dentist's side",
    "version": "1.0.0",
    "title": "Dentist Microservice",
    "contact": {
      "email": "z5128774@ad.unsw.edu.au"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  },
  "basePath": "/v1",
  "tags": [
    {
      "name": "Dentist",
      "description": "Everything about dentist"
    }
  ],
  "paths": {
    "/locations": {
      "get": {
        "tags": [
          "Dentist"
        ],
        "summary": "Get all location of clinic",
        "operationId": "clinic_loc",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "401": {
            "description": "Unauthorized API key"
          },
          "default": {
            "description": "Something is wrong"
          }
        },
        "security": [
          {
            "api_key": []
          }
        ]
      }
    },
    "/dentists": {
      "get": {
        "tags": [
          "Dentist"
        ],
        "summary": "Get all available dentist",
        "operationId": "get_dentist_ls",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "offset",
            "in": "query",
            "type": "integer",
            "minimum": 0,
            "default": 0,
            "required": true
          },
          {
            "name": "limit",
            "in": "query",
            "type": "integer",
            "minimum": 1,
            "default": 10,
            "required": true
          },
          {
            "name": "name",
            "in": "query",
            "type": "string"
          },
          {
            "name": "location",
            "in": "query",
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Dentist"
              }
            }
          },
          "401": {
            "description": "Unauthorized API key"
          },
          "default": {
            "description": "Something is wrong"
          }
        },
        "security": [
          {
            "api_key": []
          }
        ]
      }
    },
    "/dentists/{dentistId}": {
      "get": {
        "tags": [
          "Dentist"
        ],
        "summary": "Get a dentist detail information",
        "operationId": "get_dentist",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "dentistId",
            "in": "path",
            "type": "string",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/Dentist"
            }
          },
          "401": {
            "description": "Unauthorized API key"
          },
          "404": {
            "description": "dentist not found"
          },
          "default": {
            "description": "Something is wrong"
          }
        },
        "security": [
          {
            "api_key": []
          }
        ]
      }
    },
    "/dentists/{dentistId}/timeslots": {
      "get": {
        "tags": [
          "Dentist"
        ],
        "operationId": "get_dentist_timeslot",
        "summary": "Get the dentist available timeslot",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "dentistId",
            "in": "path",
            "type": "string",
            "required": true
          },
          {
            "name": "start_date",
            "type": "string",
            "format": "date",
            "in": "query",
            "required": true
          },
          {
            "name": "end_date",
            "type": "string",
            "format": "date",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/Timeslot"
            }
          },
          "401": {
            "description": "Unauthorized API key"
          },
          "404": {
            "description": "dentist not found"
          },
          "default": {
            "description": "Something is wrong"
          }
        },
        "security": [
          {
            "api_key": []
          }
        ]
      }
    }
  },
  "securityDefinitions": {
    "api_key": {
      "type": "apiKey",
      "name": "api_key",
      "in": "header"
    }
  },
  "definitions": {
    "Dentist": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "location": {
          "type": "string"
        },
        "work_days": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "Monday",
              "Tuesday",
              "Wednesday",
              "Thursday",
              "Friday",
              "Saturday",
              "Sunday"
            ]
          }
        },
        "specialities": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "public health dentistry",
              "dento-maxillofacial radiology",
              "endodontics",
              "forensic odontology",
              "oral and maxillofacial surgery",
              "oral medicine",
              "oral and maxillofacial pathology",
              "oral surgery",
              "orthodontics",
              "paediatric dentistry",
              "periodontics",
              "prosthodontics",
              "special needs dentistry"
            ]
          }
        }
      }
    },
    "Timeslot": {
      "type": "object",
      "properties": {
        "date": {
          "type": "string",
          "format": "date"
        },
        "time": {
          "type": "string",
          "description": "starting time, 1 hour timeslot",
          "pattern": "^\\d{2}(A|P)M$"
        },
        "status": {
          "type": "string",
          "pattern": "^(Available|Reserved)$"
        }
      }
    }
  }
}